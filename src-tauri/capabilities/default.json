{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "stronghold:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://www.classcharts.com/*"
        }
      ]
    },
    "sql:default",
    "sql:allow-execute"
  ]
}